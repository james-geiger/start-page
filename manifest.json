{
    "manifest_version": 2,
    "name": "New Tab Start Page",
    "version": "1.0",
    "description": "A custom new tab page with favorites and local weather.",
    "icons": {
        "96": "icon.png"
     },
     "chrome_url_overrides": {
        "newtab": "index.html"
     },
     "permissions": [
        "tabs",
        "storage"
      ]
  }